BUBBLE SORT O(n^2) // O(1)
static void bubble(int arr[]){
  boolean swapped = false;
        for(int i =0; i < arr.length-1; i++){
            for (int j = 1; j < arr.length - i; j++){
                if(arr[j] < arr[j-1]){
                    int temp = arr[j];
                    arr[j] = arr[j-1];
                    arr[j-1] = temp;
                    swapped = true;
                }}
                  if(!swapped){
                    break;}
                  }}
